# Flask Configuration
SECRET_KEY=change-this-to-a-random-secret-key-min-32-characters
FLASK_ENV=development

# Database Configuration
# For local development (SQLite)
DATABASE_URL=sqlite:///edudap.db

# For production (PostgreSQL) - Render will provide this automatically
# DATABASE_URL=postgresql://user:password@host:port/database

# File Upload Configuration
UPLOAD_FOLDER=uploads
MAX_UPLOAD_SIZE=10485760

# Server Configuration (Render sets this automatically)
# PORT=5000
